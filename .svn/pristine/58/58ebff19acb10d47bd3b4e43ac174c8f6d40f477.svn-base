/*  02 - Main Structure -------------------------------------------------- */
/*------------------------------------------------------------------- */


#wrapper {	
	margin: 0 auto;
	width: 1100px;		
	padding: 0 0 50px 0;	
}


#container {	
	margin: 0 auto;
	padding: 0;
	background: #fff;
	margin: 0 0 0 0;		
}

#main {
	padding: 0px 18px 0 0px;
	background: #fff url(images/main_bkg.gif) right top no-repeat;	
}

#content {
	width: 732px;	
	margin: 0;
	padding: 15px 0 25px 0;	
	float: left;
	background: transparent url(images/content_top_shadow.png) right top no-repeat;
}

#content .inside{
	padding: 20px 30px 0 30px;
}

#sidebar {
	width: 350px;
	float: right;
	margin: 0;	
	background: #ededed url(images/sidebar_top_shadow.png) left top repeat-x;	
}

#sidebar .inside {
	padding: 35px 30px 30px 36px;
	background: transparent url(images/sidebar_bottom_corner.gif) left bottom no-repeat;	
}

/* Clearfix ----------- */

.clearfix:after {
    content: " ";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}

.clearfix {
    display: inline-block;
}

html[xmlns] .clearfix {
    display: block;
}

* html .clearfix {
    height: 1%;
}



